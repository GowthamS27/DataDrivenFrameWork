<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Login suite">
	<test name="BothCorrect">
		<parameter name="username" value="student"></parameter>
		<parameter name="password" value="Password123"></parameter>
		<classes>
			<class name="org.UsingTestNGParameters.BothCorrect" />
		</classes>
	</test>
	<test name="BothIncorrect">
		<parameter name="username" value="dummy"></parameter>
		<parameter name="password" value="dummy"></parameter>
		<classes>
			<class name="org.UsingTestNGParameters.BothIncorrect" />
		</classes>
	</test>
	<test name="CorrectPassword">
		<parameter name="username" value="dummy"></parameter>
		<parameter name="password" value="Password123"></parameter>
		<classes>
			<class name="org.UsingTestNGParameters.CorrectPassword" />
		</classes>
	</test>
	<test name="CorrectUserName">
		<parameter name="username" value="student"></parameter>
		<parameter name="password" value="dummy"></parameter>
		<classes>
			<class name="org.UsingTestNGParameters.CorrectUserName" />
		</classes>
	</test>
</suite>